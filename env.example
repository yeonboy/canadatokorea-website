# Next.js 환경 변수
NEXT_PUBLIC_SITE_URL=https://canadatokorea.com
NEXT_PUBLIC_GA_ID=G-XXXXXXXXXX

# AWS 설정
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1
S3_BUCKET=canadatokorea-website

# Cloudflare 설정 (CloudFront 대신)
CLOUDFLARE_API_TOKEN=your_cloudflare_api_token
CLOUDFLARE_ZONE_ID=your_cloudflare_zone_id
CLOUDFLARE_EMAIL=your_cloudflare_email

# 한국 오픈 데이터 API (무료)
SEOUL_TOPIS_API_KEY=your_seoul_topis_api_key
SEOUL_OPENDATA_API_KEY=your_seoul_opendata_api_key

# Google 서비스 (무료)
GOOGLE_SERVICE_ACCOUNT_KEY=your_google_service_account_json
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# 관리자 인증 (Basic Auth)
ADMIN_USERNAME=admin
ADMIN_PASSWORD=your_secure_password

# 개발 환경
NODE_ENV=production
